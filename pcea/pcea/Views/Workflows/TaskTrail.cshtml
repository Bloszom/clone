@model IEnumerable<pcea.Models.WorkflowManager>


<div class="row">
    <div class="col-lg-12 text-center">
        <small class="text-danger text-center">Find below work done by other staffs on this task</small>
    </div>
    <div class="col-lg-12">
        <div class="table-responsive-sm p-0 text-small">
            <table class="table table-striped table-sm">
                <thead>
                    <tr class="text-center">
                        <th>
                            @*@Html.DisplayNameFor(model => model.)*@
                            Name
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.RoleId)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.DateAssigned)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.ActionId)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.Remarks)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.DateCompleted)
                        </th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr class="text-center">
                            <td>
                                @Html.DisplayFor(modelItem => item.AdminFullName)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.RoleId)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.DateAssigned)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.ActionId)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Remarks)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.DateCompleted)
                            </td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
    </div>
