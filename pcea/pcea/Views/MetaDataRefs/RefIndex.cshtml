@model IEnumerable<pcea.Models.MetaDataRef>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ReferenceId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MetaDataType)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ReferenceId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MetaDataType)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.RecId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.RecId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.RecId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
